{
  "expo": {
    "name": "Kuroclean",
    "slug": "kuroclean",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#2D5A3D"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kuroclean.app"
    },
    "android": {
      "backgroundColor": "#2D5A3D",
      "package": "com.kuroclean.app"
    },
    "plugins": [
      "expo-font"
    ]
  }
}
